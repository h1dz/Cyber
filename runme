ffuf -w livedomains1.txt:DOMAIN -u "https://FUZZ-admin.DOMAIN" -w /usr/share/wordlists/seclists/Discovery/DNS/subdomains-top1million-110000.txt -ic -mc 200 -c -v -fs 0 -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' | tee -a -i subdomains1.txt
cat livedomains1.txt | gau --mc 200 | tee -a -i subdomains1.txt 
printf "\n Completed: Sub Domain Brute-Force, Pass 2/2 \n"

#Find live hosts
cat livedomains1.txt subdomains1.txt > subdomains2.txt | sort subdomains2.txt | uniq -u > live23.txt
#Grep only URLs
cat live23.txt | grep -o 'https://[A-Za-z0-9.-]*/' > live24.txt
/home/kali/bounty/httpx -l live24.txt -mc 200 -follow-redirects -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' -o livedomains00.txt
#Check each url contains the TLD and is in scope
python3 /home/kali/Tools/web/SDBF/markov.py -n 100 /home/kali/bounty/livedomains00.txt > scanme1.txt; /home/kali/bounty/httpx -l scanme1.txt -mc 200 -fl 0 -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' -o livedomains555.txt
cat livedomains555.txt livedomains333.txt > sortthis123.txt; sort sortthis123.txt | uniq -u > livedomains000.txt
cat livedomains000.txt | grep -oE "https?://[^/]+\.$1\.$2./" > livedomains.txt
printf "\n Completed: Sub Domain Enumeration! \n"

clear
printf "Finished bruteforcing Sub Domains! \n"

