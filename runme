printf "Starting to bruteforce Directories and Files! \n"
#Brute force dirs:
ffuf -w livedomains.txt:HOST -w dirs-bf.txt:PATH -u HOST/PATH/FUZZ -recursion -recursion-depth 10 -ic -mc 200 -c -v -fs 0 -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' | tee -a -i directories111.txt
cat directories111.txt | grep -o 'https://[A-Za-z0-9.-]*' > directories55.txt; cat directories55.txt | urless > directories1.txt

#Run all found dirs again:
ffuf -w directories1.txt:HOST -w dirs-bf.txt:PATH -u HOST/PATH/FUZZ -recursion -recursion-depth 10 -ic -mc 200 -c -v -fs 0 -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' | tee -a -i directories112.txt
cat directories112.txt | grep -o 'https://[A-Za-z0-9.-]*' > directories121.txt
printf "\n Completed: Directory and File Brute-Force, PASS 1/2 \n"
#Merge and find files:
cat directories121.txt directories1.txt > new44.txt; sort new44.txt | uniq -u > directories2.txt
ffuf -w directories2.txt:HOST -w  dirs-bf.txt:PATH -u HOST/PATH/FUZZ -ic -mc 200 -c -v -fs 0 -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' -o files121.txt
cat files121.txt | grep -o 'https://[A-Za-z0-9.-]*' > files1.txt

printf "\n Completed: Directory and File Brute-Force, PASS 2/2 \n"
#Check for wp-config
grep "wp-config.php" files1.txt >> files2.txt
grep "wp-config.php" directories2.txt >> files2.txt

#Check all within scope so far
cat directories2.txt | grep -oE "https?://[^/]+\.$1\.$2.*" > directories77.txt; cat directories77.txt | urless > directories.txt
cat files2.txt | grep -oE "https?://[^/]+\.$1\.$2.*" > files3.txt
cat files1.txt files3.txt > sort1.txt; sort sort1.txt | uniq -u > files88.txt; cat files88.txt | urless > files.txt

clear
printf "Finished bruteforcing Directories and Files! \n"
printf "Starting to find juicy stuff! \n"
