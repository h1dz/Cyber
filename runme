printf "Found many files, continuing with script... \n"

#Find more js files from above
cat editfiles1.txt | urless > editfiles123.txt
for D in $(cat editfiles123.txt); do curl -sk -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' "$D" | grep -Eo 'https?://[^ ]+\.js' && echo "$D" >> javascript.txt; done;
sudo python /home/kali/Tools/web/LinkFinder/linkfinder.py -i /home/kali/bounty/javascript.txt -d -o endpoints7.txt
sudo python3 /home/kali/Tools/web/SecretFinder/SecretFinder.py -i /home/kali/bounty/javascript.txt -e -g 'jquery;bootstrap;api.google.com' -o endpoints8.txt
sudo python3 /home/kali/Tools/web/xnLinkFinder/xnLinkFinder.py -i /home/kali/bounty/javascript.txt -sf $3 -d 5 -sp "https://www.$3" >> endpoints823.txt
cat endpoints7.txt endpoints8.txt endpoints823.txt > new9.txt; sort new9.txt | uniq -u > morejs2.txt

#Check all with scope of the TLD
cat morejs2.txt | grep -oE "https?://[^/]+\.$1\.$2.*" > check7.txt
cat check7.txt juicy1.txt > new8.txt; sort new8.txt | uniq -u > juicy555.txt; cat juicy555.txt | urless > juicy1.txt

#Check for wp-config
grep "wp-config.php" juicy1.txt >> files.txt
for D in $(cat javascript.txt); do curl -sk -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' "$D" | grep "wp-config.php" && echo "$D" >> files.txt; done;

#Check for id_rsa
grep "id_rsa" juicy1.txt >> files.txt
