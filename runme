cat endpoints3.txt endpoints4.txt > new1.txt; sort new1.txt | uniq -u > almostjs.txt
cat almostjs.txt juicy0.txt > new2.txt; sort new2.txt | uniq -u > juicy111.txt; cat juicy111.txt | urless > juicy18.txt
sudo python3 /home/kali/Tools/web/xnLinkFinder/xnLinkFinder.py -i /home/kali/bounty/juicy18.txt -sf $3 -d 5 -sp "https://www.$3" >> juicy1.txt
