#Sub Domains:
clear
domain_name="$1"
domain_dot="$2"
full_domain="$3"
local_ip=$(wget -qO - https://api.ipify.org)
printf "Starting reconnaisance for: $3 \n"
gau --mc 200 $3 | tee -a -i subdomains0.txt
printf "\n Completed: gau \n"
echo $3 | assetfinder --subs-only | tee -a -i subdomains0.txt
printf "\n Completed: assetfinder \n"
subfinder -d $3 -duc | tee -a -i subdomains0.txt
printf "\n Completed: subfinder \n"
sublist3r -d $3 -v | tee -a -i subdomains0.txt
printf "\n Completed: sublister \n"
amass enum --passive -d $3 | tee -a -i subdomains0.txt
printf "\n Completed: amass \n"

ffuf -u "https://FUZZ.$3/" -w dirs-bf.txt -ic -mc 200 -c -v -fs 0 -H 'User-Agent: Mozilla Firefox Mozilla/5.0 ---- HackerOne ---- Bug Bounty Platform' -H 'X-Originating-IP: 127.0.0.1' -H 'X-Forwarded-For: 127.0.0.1' -H 'X-Forwarded: 127.0.0.1' -H 'Forwarded-For: 127.0.0.1' -H 'X-Remote-IP: 127.0.0.1' -H 'X-Remote-Addr: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Original-URL: 127.0.0.1' -H 'Client-IP: 127.0.0.1' -H 'True-Client-IP: 127.0.0.1' -H 'Cluster-Client-IP: 127.0.0.1' -H 'X-ProxyUser-Ip: 127.0.0.1' -H 'X-Custom-IP-Authorization: 127.0.0.1' | tee -a -i subdomains0.txt
